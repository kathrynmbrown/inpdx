<h1>Add an Organization</h1>
   
  <%= form_for @organization do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :contact %>
  <%= f.text_field :contact %>
  <%= f.label :tag_list, "Tags (separated by commas)" %>
  <%= f.text_field :tag_list %>
  <%= f.submit "Save organization", class: "button" %>
<% end %> 