<ul class="breadcrumbs">
  <li><a href="#">Home</a></li>
  <li class="unavailable"><a href="#">Tag Search</a></li>
  <li class="current"><a href="#">All Results</a></li>
</ul>

<section id="page-body">
<div class="row">
  <ul class="large-block-grid-6 small-block-grid-2">
    <% @products.each do |product| %>
      <li><%= link_to (image_tag product.avatar.url(:medium)), product_path(product) %></li>
    <% end %>
  <ul>
</div>
<div class="row">
  <ul class="large-block-grid-6 small-block-grid-2">
    <% @events.each do |event| %>
      <li><%= link_to (image_tag event.avatar.url(:medium)), event_path(event) %></li>
    <% end %>
  </ul>
<div>
<div class="row">
  <ul class="large-block-grid-6 small-block-grid-2">
    <% @companies.each do |company| %>
      <li><%= link_to (image_tag company.avatar.url(:medium)), company_path(company) %></li>
    <% end %>
  </ul>
</div>
<div class="row">
  <ul class="large-block-grid-6 small-block-grid-2">
    <% @restaurants.each do |restaurant| %>
      <li><%= link_to (image_tag restaurant.avatar.url(:medium)), restaurant_path(restaurant) %></li>
    <% end %>
  </ul>
</div>
</section>
